  "exclude": [
    "file://*/*.mp4*",
    "file://*/*.webm*",
    "*://accounts.youtube.com/*"
  ],
  "require": [
    "https://cdn.jsdelivr.net/npm/protobufjs/dist/light/protobuf.min.js",
    "https://cdn.jsdelivr.net/npm/hls.js/dist/hls.light.min.js",
    "https://cdn.jsdelivr.net/npm/animejs@3/lib/anime.min.js",
